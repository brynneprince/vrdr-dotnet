<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <RootNamespace>FhirDeathRecord</RootNamespace>
    <DocumentationFile>DeathRecord.xml</DocumentationFile>
    <PackageId>FHIRDeathRecord</PackageId>
    <Version>0.9.0</Version>
    <ProjectUrl>https://github.com/nightingaleproject/csharp-fhir-death-record</ProjectUrl>
    <RepositoryUrl>https://github.com/nightingaleproject/csharp-fhir-death-record</RepositoryUrl>
    <Title>FHIR Death Record</Title>
    <PackageDescription>A FHIR Death Record model for producing and consuming FHIR death records.</PackageDescription>
    <Authors>aholmes@mitre.org</Authors>
    <PackageLicenseUrl>https://github.com/nightingaleproject/csharp-fhir-death-record/blob/master/LICENSE.md</PackageLicenseUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Hl7.Fhir.STU3" Version="1.0.0" />
  </ItemGroup>
</Project>
